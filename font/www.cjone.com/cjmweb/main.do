        async : true,
        type : "POST",
        data : {},
        cache : false,
        success : function(objJson) {
            var item = JSON.parse(objJson.onmart);
            if(item.RESULT_CODE == "0000") {
                $('.onmart .img_wrap img').attr("src", item.P_IMG_URL);
                $('#onmart_title').text(item.TITLE);
                if(item.N_PRICE == item.C_PRICE){
                    $('#onmart_ori_price').text("");    
                }else{
                    $('#onmart_ori_price').text(item.N_PRICE);
                }
                $('.onmart .price').text(item.C_PRICE);
                $('.onmart .btn_wrap a').attr("href", item.P_URL);

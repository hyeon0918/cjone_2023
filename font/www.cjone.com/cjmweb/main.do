function gaAction(gaType, part, idx){
    var dt = new Date();
    var month = dt.getMonth()+1;
    if(month < 10){
        month = "0"+ month;
    }
    var day = dt.getDate();
    var year = ''+dt.getFullYear();
    var dayStr = year + month + day;
    
    if(gaType == "MB001"){
        ga('send', 'event', '홈페이지 메인 _ 프로모션', idx+'_프로모션', dayStr+'_템플릿A_'+part);  
    }else if(gaType == "MB002"){
        ga('send', 'event', '홈페이지 메인 _ 프로모션', idx+'_프로모션', dayStr+'_템플릿B_'+part);
    }else if(gaType == "MB013"){
        ga('send', 'event', '홈페이지 메인 _ 프로모션', idx+'_프로모션', dayStr+'_템플릿C_'+part);
    }else if(gaType == "CGV"){
        ga('send', 'event', '홈페이지 메인 _ 제휴 컨텐츠', '컨텐츠 '+idx+'_CGV', '[CGV]_'+dayStr+'_'+part); //part:컨텐츠제목
    }else if(gaType == "BLOG"){
        ga('send', 'event', '홈페이지 메인 _ 제휴 컨텐츠', '컨텐츠_블로그', '[블로그]_'+dayStr+'_'+part); //part:컨텐츠제목
    }else if(gaType == "MB021"){
        ga('send', 'event', '홈페이지 메인 _ CJ ONE 금융타운', '금융_프로모션', dayStr+'_'+idx); //part:컨텐츠제목
    }else if(gaType == "FACEBOOK"){
        ga('send', 'event', '홈페이지 메인 _ 제휴 컨텐츠', '컨텐츠_페이스북', '[페이스북]_'+dayStr+'_'+part); //part:컨텐츠제목
    }
}

function checkGiftCard(){
    $.ajax({
        url : "/cjmweb/getTotalGiftCardAmount.do",
        dataType : "json",
        async : true,
        type : "POST",
        data : {},
        cache : false,
        success : function(objJson) {
            var rstCd = objJson.result.ResultCode;
            var rstMsg = objJson.result.ResultMsg;
            if( rstCd == "00000" ){
                var amount = mainAddComma(parseInt(objJson.result.comm_total_amount,10));
                var html = '<a href="https://www.cjone.com/cjmweb/cash/cashmain.do"><span class="count" data-control="randomNum" data-val="0"><span class="d-digit" data-val="0">'+amount+'</span></span><span class="txt">원</span></a>';
                $('#mainDivGiftInfo').html(html);
            }else{
                alert(rstMsg);
            }
        }
    });
}

function mainAddComma(value){
    var regComma = /(\d+)(\d{3})/;
    value += '';
    var x = value.split('.'),
        x1 = x[0],
        x2 = x.length > 1 ? '.' + x[1] : '';

    while (regComma.test(x1)) {
        x1 = x1.replace(regComma, '$1' + ',' + '$2');
    }
    return x1 + x2;
}
